{
  "name": "aoc-signal",
  "version": "1.0.6",
  "description": "This is a RTC library based on leancloud js-realtime-sdk.",
  "main": "./lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "test": "cross-env BABEL_ENV=commonjs mocha --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch",
    "test:examples": "node examples/",
    "cover": "cross-env BABEL_ENV=commonjs istanbul cover _mocha -- --compilers js:babel-register --recursive",
    "lint": "tslint --format prose src/**/*.ts test/**/*.ts",
    "build": "tsc",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run build",
    "benchmark:receiver": "babel-node benchmark/receiver.js",
    "benchmark:receiver:zx": "SERVER=wss://rtm-global-in.leancloud.cn babel-node benchmark/receiver.js"
  },
  "typings": "./lib/index.d.ts",
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Travelport-Ukraine/npm-module-boilerplate.git"
  },
  "keywords": [
    "boilerplate",
    "npm",
    "module",
    "node",
    "javascript"
  ],
  "author": "byrne.liang@alo7.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Travelport-Ukraine/npm-module-boilerplate/issues"
  },
  "homepage": "https://github.com/Travelport-Ukraine/npm-module-boilerplate#readme",
  "dependencies": {
    "debug": "^3.1.0",
    "immutable": "^3.8.2",
    "leancloud-realtime": "next"
  },
  "devDependencies": {
    "@types/debug": "^0.0.30",
    "@types/node": "^9.4.1",
    "babel": "^6.5.2",
    "babel-cli": "^6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.13.0",
    "chai": "^3.5.0",
    "cross-env": "^3.0.0",
    "eslint": "^3.6.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "istanbul": "^1.0.0-alpha",
    "mocha": "^3.0.2",
    "rimraf": "^2.5.4",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1"
  }
}
