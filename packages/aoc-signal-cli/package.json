{
  "name": "aoc-signal-cli",
  "version": "1.0.0",
  "description": "This is a cli application to do leancloud benchmark.",
  "main": "./bin/leancloud-benchmark.js",
  "scripts": {
    "clean": "rimraf lib",
    "test": "",
    "test:watch": "npm test -- --watch",
    "cover": "cross-env BABEL_ENV=commonjs istanbul cover _mocha -- --compilers js:babel-register --recursive",
    "lint": "eslint src",
    "build": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run build",
    "benchmark:receiver": "babel-node benchmark/receiver.js",
    "benchmark:receiver:zx": "SERVER=wss://rtm-global-in.leancloud.cn babel-node benchmark/receiver.js"
  },
  "files": [
    "lib",
    "src",
    "bin"
  ],
  "bin": {
    "leancloud-benchmark": "./src/bin/index.js"
  },
  "author": "byrne.liang@alo7.com",
  "license": "MIT",
  "homepage": "",
  "dependencies": {
    "aoc-signal": "*"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^3.5.0",
    "cross-env": "^3.0.0",
    "eslint": "^3.6.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "istanbul": "^1.0.0-alpha",
    "mocha": "^3.0.2",
    "rimraf": "^2.5.4"
  }
}
